#include <iostream>
#include <iomanip>
using namespace std;
int
main ()
{
  string s;
  int n;
  float cgpa;
  char a;
  cin >> s;
  cin >> n;
  cin >> cgpa;
  cin >> a;
  cout << "Name:" << s << endl;
  cout << "Age:" << n << endl;
  cout << fixed << setprecision (2);
  cout << "CGPA:" << cgpa << endl;
  cout << "Grade:" << a << endl;
}
